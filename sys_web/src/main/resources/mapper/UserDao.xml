<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.library_online.sys_dao.UserDao">
    <!--登录-->
    <select id="login" parameterType="java.lang.String" resultType="int">
        select count(*) from user where username = #{username} and user_pwd = #{userPwd}
    </select>

</mapper>